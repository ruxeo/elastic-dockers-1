    </span><span class=3D"kwd">healthcheck:
</span><span class=3D"pln">      </span><span class=3D"kwd">test:
</span><span class=3D"pln">        [
            </span><span class=3D"str">"CMD-SHELL"</span><span class=3D"pln=
">,
            </span><span class=3D"str">"curl -s -I http://localhost:3002 | =
grep -q 'HTTP/1.1 302 Found'"</span><span class=3D"pln">,
        ]
      </span><span class=3D"kwd">interval: </span><span class=3D"pln">10s
      </span><span class=3D"kwd">timeout: </span><span class=3D"pln">10s
      </span><span class=3D"kwd">retries: </span><span class=3D"pln">120

</span><span class=3D"kwd">volumes:
</span><span class=3D"pln">  </span><span class=3D"kwd">certs:
</span><span class=3D"pln">    </span><span class=3D"kwd">driver: </span><s=
pan class=3D"pln">local
  </span><span class=3D"kwd">enterprisesearchdata:
</span><span class=3D"pln">    </span><span class=3D"kwd">driver: </span><s=
